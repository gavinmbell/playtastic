(defproject @@PROJECT@@ "@@VERSION@@"
  :description "Code Playtastical Stuff: Do some cool stuff"
  :url "@@REPO_SITE@@"
  :license {:name "ECLIPSE License"
            :url "@@REPO_SITE@@/blob/master/LICENSE"}
  :dependencies [[org.clojure/clojure "1.8.0"]
                 [org.clojure/tools.logging "0.4.0"]
                 [table "0.5.0"]]
  :aot [@@PROJECT@@.core]
  :main @@PROJECT@@.core
  :manifest {"Organization" "@@ORGANIZATION@@"
             "Author" "@@AUTHOR@@"
             "Repo-Site" "@@REPO_SITE@@"
             "Project" "@@PROJECT@@"
             "Version" "@@VERSION@@"
             "Branch" "@@BRANCH@@"
             "Commit" "@@COMMIT@@"
             "Last-Commit-Author" "@@COMMIT_AUTHOR@@"
             "Last-Commit-Date" "@@COMMIT_DATE@@"
             "Build-Date" "@@BUILD_DATE@@"
             "Profile" "@@PROFILE@@"
             "Release-Name" "@@RELEASE_NAME@@"}
  :plugins [[lein-codox "0.10.3"]]
  :codox {:sources-paths ["src"] :doc-files ["README.md", "doc/discussion.md"]}
)
